# Example environment variables for local development
# Copy this file to .env and fill in your values
# Note: .env is gitignored
#
# Environment variable naming convention:
# - Mirrors YAML structure with underscores: section_field
# - Arrays use indexed suffix: cors_origins_0, cors_origins_1
# - Nested objects: database_replicas_0_host, database_replicas_0_port

# Environment selection
ENV=development

# Database configuration
database_host=localhost
database_port=5432
database_database=agentlab_dev
database_user=agentlab
database_password=your_password_here
database_max_conns=10
database_min_conns=2

# Server configuration
server_port=8080
server_host=localhost

# Logging configuration
logging_level=debug
logging_format=text

# Example: Array configuration (if CORS origins were configured)
# cors_origins_0=http://localhost:3000
# cors_origins_1=http://localhost:4000

# Example: Array of objects (if database replicas were configured)
# database_replicas_0_host=db1
# database_replicas_0_port=5432
# database_replicas_1_host=db2
# database_replicas_1_port=5432
