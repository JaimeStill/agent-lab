  "components": {
    "schemas": {
      "Agent": {
        "type": "object",
        "properties": {
          "config": {
            "type": "object",
            "description": "go-agents AgentConfig as JSON (includes embedded provider config)"
          },
          "created_at": {
            "type": "string",
            "format": "date-time"
          },
          "id": {
            "type": "string",
            "format": "uuid"
          },
          "name": {
            "type": "string"
          },
          "updated_at": {
            "type": "string",
            "format": "date-time"
          }
        }
      },
      "AgentPageResult": {
        "type": "object",
        "properties": {
          "data": {
            "type": "array",
            "description": "Array of agents"
          },
          "page": {
            "type": "integer",
            "description": "Current page number"
          },
          "page_size": {
            "type": "integer",
            "description": "Results per page"
          },
          "total": {
            "type": "integer",
            "description": "Total number of results"
          },
          "total_pages": {
            "type": "integer",
            "description": "Total number of pages"
          }
        }
      },
      "ChatRequest": {
        "type": "object",
        "properties": {
          "options": {
            "type": "object",
            "description": "Optional agent options override"
          },
          "prompt": {
            "type": "string",
            "description": "User prompt"
          },
          "token": {
            "type": "string",
            "description": "Optional authentication token (for Azure providers)"
          }
        },
        "required": [
          "prompt"
        ]
      },
      "ChatResponse": {
        "type": "object",
        "properties": {
          "response": {
            "type": "string",
            "description": "Agent response text"
          }
        }
      },
      "CreateAgentCommand": {
        "type": "object",
        "properties": {
          "config": {
            "type": "object",
            "description": "go-agents AgentConfig as JSON"
          },
          "name": {
            "type": "string",
            "example": "gpt-4o"
          }
        },
        "required": [
          "name",
          "config"
        ]
      },
      "CreateProviderCommand": {
        "type": "object",
        "properties": {
          "config": {
            "type": "object",
            "description": "go-agents ProviderConfig as JSON"
          },
          "name": {
            "type": "string",
            "example": "ollama"
          }
        },
        "required": [
          "name",
          "config"
        ]
      },
      "EmbedRequest": {
        "type": "object",
        "properties": {
          "input": {
            "type": "string",
            "description": "Text to embed"
          },
          "options": {
            "type": "object",
            "description": "Optional embedding options"
          },
          "token": {
            "type": "string",
            "description": "Optional authentication token"
          }
        },
        "required": [
          "input"
        ]
      },
      "EmbedResponse": {
        "type": "object",
        "properties": {
          "embedding": {
            "type": "array",
            "description": "Embedding vector"
          }
        }
      },
      "PageRequest": {
        "type": "object",
        "properties": {
          "page": {
            "type": "integer",
            "description": "Page number (1-indexed)",
            "example": 1
          },
          "page_size": {
            "type": "integer",
            "description": "Results per page",
            "example": 20
          },
          "search": {
            "type": "string",
            "description": "Search query"
          },
          "sort": {
            "type": "string",
            "description": "Comma-separated sort fields. Prefix with - for descending. Example: name,-created_at"
          }
        }
      },
      "Provider": {
        "type": "object",
        "properties": {
          "config": {
            "type": "object",
            "description": "go-agents ProviderConfig as JSON"
          },
          "created_at": {
            "type": "string",
            "format": "date-time"
          },
          "id": {
            "type": "string",
            "format": "uuid"
          },
          "name": {
            "type": "string"
          },
          "updated_at": {
            "type": "string",
            "format": "date-time"
          }
        }
      },
      "ProviderPageResult": {
        "type": "object",
        "properties": {
          "data": {
            "type": "array",
            "description": "Array of providers"
          },
          "page": {
