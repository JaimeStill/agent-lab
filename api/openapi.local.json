{
  "openapi": "3.1.0",
  "info": {
    "title": "Agent Lab API",
    "version": "0.1.0",
    "description": "Containerized web service platform for building and orchestrating agentic workflows."
  },
  "servers": [
    {
      "url": "http://localhost:8080"
    }
  ],
  "paths": {
    "/api/agents": {
      "get": {
        "summary": "List agents",
        "description": "Returns a paginated list of agents with optional filtering and sorting",
        "tags": [
          "Agents"
        ],
        "parameters": [
          {
            "name": "page",
            "in": "query",
            "description": "Page number (1-indexed)",
            "schema": {
              "type": "integer"
            }
          },
          {
            "name": "page_size",
            "in": "query",
            "description": "Results per page",
            "schema": {
              "type": "integer"
            }
          },
          {
            "name": "search",
            "in": "query",
            "description": "Search query (matches name)",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "sort",
            "in": "query",
            "description": "Comma-separated sort fields. Prefix with - for descending",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "name",
            "in": "query",
            "description": "Filter by agent name (contains)",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Paginated list of agents",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/AgentPageResult"
                }
              }
            }
          }
        }
      },
      "post": {
        "summary": "Create agent",
        "description": "Validates and stores a new agent configuration",
        "tags": [
          "Agents"
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/CreateAgentCommand"
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "Agent created",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Agent"
                }
              }
            }
          },
          "400": {
            "description": "",
            "$ref": "#/components/responses/BadRequest"
          },
          "409": {
            "description": "",
            "$ref": "#/components/responses/Conflict"
          }
        }
      }
    },
    "/api/agents/search": {
      "post": {
        "summary": "Search agents",
